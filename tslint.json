{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "interface-name": [true, "never-prefix"],
    "no-console": [true],
    "one-variable-per-declaration": [false],
    "prefer-for-of": [false],
    "class-name": [false],
    "interface-over-type-literal": [false]
  },
  "rulesDirectory": []
}
